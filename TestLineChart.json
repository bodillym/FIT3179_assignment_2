{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "Practice Line Graph",
  "data": {"url": "https://bodillym.github.io/FIT3179_assignment_2/Melanoma_by_states.csv"},
  "mark": {
    "type": "area",
    "line": {
      "color": "darkgreen"
    },
    "color": {
      "x1": 1,
      "y1": 1,
      "x2": 1,
      "y2": 0,
      "gradient": "linear",
      "stops": [
        {
          "offset": 0,
          "color": "white"
        },
        {
          "offset": 1,
          "color": "darkgreen"
        }
      ]
    }
  },
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year"},
    "y": {"aggregate": "sum", "field": "Count", "type": "quantitative"}
    }
  }
"config" : {}
}



